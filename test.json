{
  "id": "dashboard",
  "name": "Dashboard",
  "url": "dashboard.innque.com",
  "version": "1.0.0",
  "schemas": [
    {
      "collection": "dashboard",
      "fields": {
        "type": {
          "type": "String",
          "required": true
        },
        "collection": {
          "type": "String",
          "required": true
        },
        "icon": {
          "type": "String"
        },
        "where": {
          "type": "Object"
        },
        "label": {
          "type": "String"
        },
        "actionLabel": {
          "type": "String"
        }
      }
    },
    {
      "collection": "roles",
      "fields": {
        "name": {
          "type": "String",
          "unique": true
        },
        "roles": {
          "type": "Relation",
          "target": "roles"
        }
      }
    },
    {
      "collection": "users",
      "fields": {
        "username": {
          "type": "String",
          "unique": true,
          "required": true,
          "lowercase": true,
          "minlength": 2,
          "index": 1
        },
        "password": {
          "type": "String",
          "required": true,
          "transform": "md5",
          "minlength": 8,
          "hidden": true
        },
        "email": {
          "type": "String",
          "unique": true,
          "pattern": "^.+@.+\\..+$"
        },
        "roles": {
          "type": "Relation",
          "target": "roles"
        }
      }
    },
    {
      "collection": "sessions",
      "fields": {
        "user": {
          "type": "Pointer",
          "target": "users"
        },
        "expiresAt": {
          "type": "Date"
        },
        "masterKey": {
          "type": "String",
          "hidden": true
        }
      },
      "permissions": {
        "find": [
          "*"
        ],
        "get": [
          "*"
        ]
      }
    }
  ]
}